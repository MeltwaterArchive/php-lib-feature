{
    "name": "datasift/feature",
    "description": "Library tool for managing feature flags",
    "homepage": "https://github.com/datasift/php-lib-feature",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Macnamara",
            "email": "nathan.macnamara@datasift.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/options-resolver": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "mockery/mockery": "^0.9.4",
        "predis/predis": "^1.0",
        "sensiolabs/consul-php-sdk": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DataSift\\Feature\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataSift\\Feature\\Test\\": "tests/"
        }
    },
    "suggest": {
        "predis/predis": "Redis driver",
        "sensiolabs/consul-php-sdk": "Consul driver"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
